#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/touch/bin/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/touch/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/node_modules:/mnt/c/Users/orangewood/Downloads/node_modules:/mnt/c/Users/orangewood/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/touch/bin/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/touch/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/node_modules:/mnt/c/Users/orangewood/Downloads/node_modules:/mnt/c/Users/orangewood/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/orangewood/Downloads/542raAnmn7ctfrQ54RogYR/TaskFlow/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
